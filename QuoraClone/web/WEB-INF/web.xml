<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login__info</servlet-name>
        <servlet-class>login__info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup__info</servlet-name>
        <servlet-class>signup__info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadquestion</servlet-name>
        <servlet-class>uploadquestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadanswer</servlet-name>
        <servlet-class>uploadanswer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login__info</servlet-name>
        <url-pattern>/login__info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup__info</servlet-name>
        <url-pattern>/signup__info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadquestion</servlet-name>
        <url-pattern>/uploadquestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadanswer</servlet-name>
        <url-pattern>/uploadanswer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            1
        </session-timeout>
    </session-config>
</web-app>
